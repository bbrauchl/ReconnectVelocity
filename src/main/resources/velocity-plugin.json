{
  "id": "reconnect",
  "name": "ReconnectVelocity",
  "version": "${version}",
  "description": "Reconnect your players to their last server...",
  "url": "https://www.mattmx.com/",
  "authors": ["MattMX", "bbrauchl"],

  "dependencies": [
    {
      "id": "litebans",
      "optional": true
    },
    {
      "id": "luckperms",
      "optional": true
    }
